{
  "name": "dac-204-term-project-game",
  "version": "1.0.0",
  "description": "Minimal TypeScript + HTML + CSS scaffold ready for GitHub Pages.",
  "scripts": {
    "clean": "node ./scripts/clean-dist.cjs",
    "clean:tests": "node ./scripts/clean-tests.cjs",
    "build": "npm run clean && tsc && node ./scripts/copy-static.cjs",
    "build:tests": "npm run clean:tests && tsc -p tsconfig.tests.json",
    "dev": "tsc --watch --preserveWatchOutput",
    "preview": "http-server dist -p 4173 -c-1",
    "start": "npm run build && npm run preview",
    "test": "npm run test:unit",
    "test:unit": "npm run build:tests && node ./scripts/run-tests.cjs"
  },
  "keywords": [
    "typescript",
    "frontend",
    "github-pages"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
